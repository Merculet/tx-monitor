{
  "name": "tx-monitor",
  "version": "1.0.0",
  "description": "blockchain transaction monitor",
  "scripts": {
    "start": "node ./src/App.js",
    "dev": "NODE_ENV=dev npm start",
    "consume": "NODE_ENV=dev node ./tool/KafkaConsumer",
    "cover": "node ./node_modules/istanbul/lib/cli.js cover --include-all-sources ./node_modules/jasmine/bin/jasmine.js",
    "test": "node ./node_modules/jasmine/bin/jasmine.js",
    "lint": "./node_modules/.bin/eslint -f stylish --cache --cache-location ./build/eslint/cache/ --color ./src/",
    "watch": "./node_modules/.bin/esw --watch -f stylish --cache --cache-location ./build/eslint/cache/ --color ./src/",
    "jsdoc": "./node_modules/.bin/jsdoc ./src -c .jsdoc.json"
  },
  "dependencies": {
    "composer-common": "0.19.1",
    "composer-connector-hlfv1": "0.19.1",
    "composer-wallet-filesystem": "0.19.1",
    "fabric-ca-client": "1.1.0",
    "fabric-client": "1.1.0",
    "grpc": "1.10.1",
    "js-yaml": "3.9.1",
    "kcors": "2.2.1",
    "koa": "2.3.0",
    "koa-body": "2.3.0",
    "koa-logger": "3.0.1",
    "koa-router": "7.2.1",
    "moment": "2.18.1",
    "node-rdkafka": "2.3.2",
    "qnode-beans": "1.2.4",
    "qnode-config": "1.0.8",
    "qnode-error": "1.0.6",
    "qnode-log": "1.0.4",
    "qnode-mysql": "0.9.20",
    "qnode-rest": "1.0.24",
    "sprintf-js": "1.1.1"
  },
  "devDependencies": {
    "babel": "6.23.0",
    "babel-cli": "6.24.1",
    "babel-eslint": "7.2.3",
    "babel-preset-es2015": "6.24.1",
    "babel-preset-stage-0": "6.24.1",
    "eslint": "4.3.0",
    "eslint-stylish": "0.2.0",
    "eslint-watch": "3.1.2",
    "istanbul": "1.1.0-alpha.1",
    "jasmine": "2.7.0",
    "jasmine-supertest": "1.0.0",
    "jsdoc": "3.5.4",
    "sinon": "2.4.1"
  },
  "browser": {
    "os": false
  },
  "engines": {
    "node": ">=8"
  },
